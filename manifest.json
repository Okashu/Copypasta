{
  "manifest_version": 2,

  "name": "Copypasta for Twitch",
  "short_name": "Copypasta",
  "description": "One click copypasta! Memes have never been easier!",
  "version": "0.9",

  "permissions": ["tabs","http://www.twitch.tv/"],
  "icons": {
  	"16":"icon16.png",
  	"48":"icon48.png",
  	"64":"icon64.png",
  	"128":"icon128.png"
  	},

  "content_scripts": [
    {
      "matches": ["http://www.twitch.tv/*"],
      "js":["copypasta_script.js"]
  }
  ]
}